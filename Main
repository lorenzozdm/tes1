    World1 = true
elseif game.PlaceId == 4442272183 then
    World2 = true
elseif game.PlaceId == 7449423635 then
    World3 = true
end



function StarterOn()
    OrionLib:MakeNotification({
        Name = "Loader",
        Content = "User: " .. Player.Name,
        Image = "rbxassetid://4483345998",
        Time = 5
    })
end

StarterOn()
