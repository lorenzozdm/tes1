local Tab = Window:MakeTab({
	Name = "Auto Farm",
	Icon = "rbxassetid://6523858422",
	PremiumOnly = false
})



local Tab2 = Window:MakeTab({
	Name = "Race V4",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})


local Tab3 = Window:MakeTab({
	Name = "Visual",
	Icon = "rbxassetid://6523858422",
	PremiumOnly = false
})


